RewriteEngine on

#DONT FORGET TO CHANGE htaccess.sample

RewriteRule ^html/hydrant/manual/?$ 					/ffla-intranet/html/hydrantapp/manual.php [L]

RewriteRule ^html/hydrant/inspection/candidates/?$ 		/ffla-intranet/html/hydrantapp/inspection_candidates.php [L]
