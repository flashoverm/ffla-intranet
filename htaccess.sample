RewriteRule ^html/ajax/privileges/([^/]*)/?$	/ffla-intranet/html/ajax/privileges.php?uuid=$1


RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}\.php -f
RewriteRule ^(.+)$ $1.php [L]